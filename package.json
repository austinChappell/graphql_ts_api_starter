{
  "name": "graphql_ts_starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && copyfiles -u 1 src/**/*.graphql dist/src",
    "dev": "NODE_PATH=./src TS_NODE_FILES=true ts-node-dev --transpile-only src",
    "db:create": "createdb covid_19_jobs_dev",
    "db:create:test": "createdb covid_19_jobs_test",
    "db:drop": "dropdb covid_19_jobs_dev --if-exists",
    "db:drop:test": "dropdb covid_19_jobs_test --if-exists",
    "db:reset": "yarn build && yarn db:drop && yarn db:setup",
    "db:reset:test": "yarn build && yarn db:drop:test && yarn db:setup:test",
    "db:seed": "NODE_PATH=./dist/src node dist/src/db/seed.js",
    "db:seed:test": "NODE_PATH=./dist/src node dist/src/db/seed_test.js",
    "db:setup": "yarn db:create && yarn db:migrate && yarn db:seed",
    "db:setup:prod": "NODE_PATH=./src yarn db:migrate",
    "db:setup:test": "yarn db:create:test && yarn db:migrate:test && yarn db:seed:test",
    "db:migrate": "node_modules/.bin/knex migrate:latest",
    "db:migrate:make": "node_modules/.bin/knex migrate:make -x ts",
    "db:migrate:test": "NODE_ENV=test node_modules/.bin/knex migrate:latest --env test",
    "db:rollback": "node_modules/.bin/knex migrate:rollback",
    "db:rollback:test": "node_modules/.bin/knex migrate:rollback --env test",
    "eslint": "node_modules/.bin/eslint src/**/*.ts --quiet",
    "gen": "NODE_PATH=./src ts-node-dev src/scripts/createTypes.ts",
    "precommit": "yarn eslint",
    "start": "yarn db:setup:prod && NODE_PATH=./dist/src node dist/src/index.js",
    "start:prod": "yarn db:setup:prod && node src/cluster.js",
    "test:ci": "yarn db:migrate:test && yarn db:seed:test && NODE_PATH=./dist/src node_modules/.bin/jest --runInBand --forceExit --verbose",
    "test": "yarn db:reset:test && NODE_PATH=./dist/src node_modules/.bin/jest --runInBand --forceExit --verbose"
  },
  "keywords": [],
  "author": "Austin Chappell",
  "license": "ISC",
  "dependencies": {
    "@gql2ts/from-schema": "^2.0.0-4",
    "apollo-server-express": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dataloader-sort": "^0.0.5",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "faker": "^4.1.0",
    "fast-csv": "^4.1.3",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.snakecase": "^4.1.1",
    "merge-graphql-schemas": "^1.7.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "pg": "^8.0.2",
    "pg-types": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^13.11.1",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.3",
    "pg-native": "^3.0.0",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
